    "@angular/router": "^4.3.1",
    "@uirouter/angular": "^1.0.0-beta.7",
    "@uirouter/core": "^5.0.5",
    "bootstrap": "^4.0.0-alpha.6",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
