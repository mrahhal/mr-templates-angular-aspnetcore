{
  "name": "mr-templates-angular-aspnetcore",
  "version": "0.1.0",
  "description": "Angular with AspNetCore",
  "private": true,
  "keywords": [
    "angular",
    "aspnetcore"
  ],
  "author": "Mohammad Rahhal @mrahhal",
  "repository": {
    "type": "git",
    "url": "https://github.com/mrahhal/mr-templates-angular-aspnetcore"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "concurrently \"gulp && npm run start:webpack\" \"set ASPNETCORE_ENVIRONMENT=Development&& dotnet run\"",
    "start:webpack": "webpack -w",
    "build": "gulp && webpack --config config/webpack.prod.js",
    "build:dev": "gulp && webpack",
    "open": "start http://localhost:12659",
    "tslint": "tslint \"src/**/*.ts\" --force"
  },
